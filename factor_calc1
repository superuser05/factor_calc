var number;
number = prompt();
function listFactors(number)
{
    var factors = [];
    if (number <= 0)
        alert("Number should be a natural number.");
    else { 
        for (var i = 1; i <= number; i++)
            {
                if (number % i == 0)
                    factors.push(i);
                else 
                    continue;
            }
        return factors;
    }
}

console.log(listFactors(number));
